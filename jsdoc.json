{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "templates": {
    "logoFile": "img/logo.png",
    "cleverLinks": true,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Mediumroast for GitHub API and SDK",
    "footer": "",
    "copyright": "Â© 2023-2025 Mediumroast, Inc. All rights reserved.",
    "navType": "inline",
    "theme": "cyborg",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "html://",
    "methodHeadingReturns": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "idInHeadings": true
  },
  "opts": {
    "template": "node_modules/ink-docstrap/template",
    "destination": "./docs",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "readme": "./README.md",
    "tutorials": "./examples"
  }
}